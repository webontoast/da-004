{% extends 'layouts/base.njk' %}

{# Body class #}
{% set bodyClass = 'contact' %}

{# Intro content #}
{% set introHeading = title %}

{% block content %}
  <main id="main-content" tabindex="-1">
    <article class="[ post ] [ h-entry ] is-2-columns">
      {% include "partials/components/intro.njk" %}
      <div class="[ post__body ] [ inner-wrapper ] [ leading-loose pad-top-900 pad-bottom-900 text-500 ] [ sf-flow ] [ e-content ]">
        {{ content | safe }}
       
      </div>
      <div class="[ post__form ] [ inner-wrapper ] [ leading-loose pad-top-900 pad-bottom-900 text-500 ] [ sf-flow ] [ e-content ]">
        <h3>Get In Touch</h3>
         {% include "partials/components/contact-form.njk" %}
      </div>
    </article>
  </main>
{% endblock %}

{{ content | safe }}